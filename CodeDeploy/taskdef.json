{
    "family": "Test",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "PostCode",
            "image": "<IMAGE>",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::174304792831:role/ECSTaskExecutionRole"
}